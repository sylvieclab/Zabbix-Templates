<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.4</version>
    <date>2021-03-06T19:20:35Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Zoes Collective Template</template>
            <name>Zoes Collective Template</name>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>CPU</name>
                </application>
                <application>
                    <name>qBittorrent</name>
                </application>
                <application>
                    <name>VMWare</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>apiversion</name>
                    <type>HTTP_AGENT</type>
                    <key>apiversion</key>
                    <delay>24h</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>qBittorrent</name>
                        </application>
                    </applications>
                    <url>{$URLQBITTORRENT}/api/v2/app/webapiVersion</url>
                    <triggers>
                        <trigger>
                            <expression>{last(#1)}&lt;&gt;&quot;2.2.1&quot;</expression>
                            <name>qBittorrent API Version Check</name>
                            <priority>INFO</priority>
                            <description>Checking to make sure the version of qBittorrent's API hasn't changed.</description>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Conection Status</name>
                    <type>DEPENDENT</type>
                    <key>qbittorrent.connection_status</key>
                    <delay>0</delay>
                    <history>7d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>qBittorrent</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.connection_status</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>transferinfo</key>
                    </master_item>
                </item>
                <item>
                    <name>Download Speed</name>
                    <type>DEPENDENT</type>
                    <key>qbittorrent.download</key>
                    <delay>0</delay>
                    <history>7d</history>
                    <trends>14d</trends>
                    <units>Bps</units>
                    <applications>
                        <application>
                            <name>qBittorrent</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.dl_info_speed</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>transferinfo</key>
                    </master_item>
                </item>
                <item>
                    <name>Download Data</name>
                    <type>DEPENDENT</type>
                    <key>qbittorrent.dwldata</key>
                    <delay>0</delay>
                    <history>7d</history>
                    <trends>14d</trends>
                    <applications>
                        <application>
                            <name>qBittorrent</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$.dl_info_data</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>transferinfo</key>
                    </master_item>
                </item>
                <item>
                    <name>qBittorrent Version</name>
                    <type>HTTP_AGENT</type>
                    <key>qbittorrent.version</key>
                    <delay>24h</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>qBittorrent</name>
                        </application>
                    </applications>
                    <url>{$URLQBITTORRENT}/api/v2/app/version</url>
                    <triggers>
                        <trigger>
                            <expression>{last()}&lt;&gt;&quot;v4.1.9.1&quot;</expression>
                            <name>qBittorrent Version Check</name>
                            <priority>INFO</priority>
                            <description>Checking to make sure the version of qBittorrent hasn't changed.</description>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>CPU utilization</name>
                    <key>system.cpu.util</key>
                    <history>7d</history>
                    <trends>14d</trends>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>CPU</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{min(5)}&gt;=70</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{min(5)}&lt;70</recovery_expression>
                            <name>CPU Utilization over 70%</name>
                            <priority>WARNING</priority>
                            <description>CPU Utilization on device is running over 70% over a 5 minute period.</description>
                        </trigger>
                        <trigger>
                            <expression>{min(5)}&gt;=80</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{min(5)}&lt;80</recovery_expression>
                            <name>CPU Utilization over 80%</name>
                            <priority>AVERAGE</priority>
                            <description>CPU Utilization on device is running over 80% over a 5 minute period.</description>
                        </trigger>
                        <trigger>
                            <expression>{min(5)}&gt;=90</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{min(5)}&lt;90</recovery_expression>
                            <name>CPU Utilization over 90%</name>
                            <priority>HIGH</priority>
                            <description>CPU Utilization on device is running over 90% over a 5 minute period.</description>
                        </trigger>
                        <trigger>
                            <expression>{min(5)}&gt;=98</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{min(5)}&lt;98</recovery_expression>
                            <name>CPU Utilization over 98%</name>
                            <priority>DISASTER</priority>
                            <description>CPU Utilization on device is running over 98% over a 5 minute period.</description>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>torrent list</name>
                    <type>HTTP_AGENT</type>
                    <key>torrentlist</key>
                    <delay>5m</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>qBittorrent</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <parameters>
                                <parameter>$..['name']</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <url>{$URLQBITTORRENT}/api/v2/torrents/info</url>
                </item>
                <item>
                    <name>qBittorrent transferInfo</name>
                    <type>HTTP_AGENT</type>
                    <key>transferinfo</key>
                    <delay>5m</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>qBittorrent</name>
                        </application>
                    </applications>
                    <url>{$URLQBITTORRENT}/api/v2/transfer/info</url>
                </item>
                <item>
                    <name>CPU usage</name>
                    <type>SIMPLE</type>
                    <key>vmware.vm.cpu.usage[{$VMWARE.URL},{$VMWARE.VM.UUID}]</key>
                    <history>7d</history>
                    <trends>14d</trends>
                    <units>Hz</units>
                    <username>{$VMWARE.USERNAME}</username>
                    <password>{$VMWARE.PASSWORD}</password>
                    <applications>
                        <application>
                            <name>VMWare</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>VMware: Host memory usage</name>
                    <type>SIMPLE</type>
                    <key>vmware.vm.memory.size.usage.host[{$VMWARE.URL},{$VMWARE.VM.UUID}]</key>
                    <history>7d</history>
                    <trends>14d</trends>
                    <units>B</units>
                    <username>{$VMWARE.USERNAME}</username>
                    <password>{$VMWARE.PASSWORD}</password>
                    <applications>
                        <application>
                            <name>VMWare</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>VMware: Memory size</name>
                    <type>SIMPLE</type>
                    <key>vmware.vm.memory.size[{$VMWARE.URL},{$VMWARE.VM.UUID}]</key>
                    <history>7d</history>
                    <trends>14d</trends>
                    <units>B</units>
                    <username>{$VMWARE.USERNAME}</username>
                    <password>{$VMWARE.PASSWORD}</password>
                    <applications>
                        <application>
                            <name>VMWare</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>VMware: Power state</name>
                    <type>SIMPLE</type>
                    <key>vmware.vm.powerstate[{$VMWARE.URL},{$VMWARE.VM.UUID}]</key>
                    <history>7d</history>
                    <trends>14d</trends>
                    <username>{$VMWARE.USERNAME}</username>
                    <password>{$VMWARE.PASSWORD}</password>
                    <applications>
                        <application>
                            <name>VMWare</name>
                        </application>
                    </applications>
                    <valuemap>
                        <name>VirtualMachinePowerState</name>
                    </valuemap>
                    <preprocessing>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <parameters>
                                <parameter>1h</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{min(2)}=0</expression>
                            <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                            <recovery_expression>{min(2)}=1</recovery_expression>
                            <name>VM is powered off</name>
                            <priority>INFO</priority>
                            <description>Data is reporting the device as powered off for the last 2 minutes. If this is expected, set the device in maintenance mode. If not, power on the device. This alert will auto resolve after getting 2 minutes of data showing a powered on state.</description>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>VMware: Uptime</name>
                    <type>SIMPLE</type>
                    <key>vmware.vm.uptime[{$VMWARE.URL},{$VMWARE.VM.UUID}]</key>
                    <delay>12h</delay>
                    <history>7d</history>
                    <trends>14d</trends>
                    <units>uptime</units>
                    <username>{$VMWARE.USERNAME}</username>
                    <password>{$VMWARE.PASSWORD}</password>
                    <applications>
                        <application>
                            <name>VMWare</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>Discover VMware clusters</name>
                    <type>SIMPLE</type>
                    <key>vmware.cluster.discovery[{$VMWARE.URL}]</key>
                    <username>{$VMWARE.USERNAME}</username>
                    <password>{$VMWARE.PASSWORD}</password>
                    <description>Discovery of clusters</description>
                    <item_prototypes>
                        <item_prototype>
                            <name>Status of &quot;{#CLUSTER.NAME}&quot; cluster</name>
                            <type>SIMPLE</type>
                            <key>vmware.cluster.status[{$VMWARE.URL},{#CLUSTER.NAME}]</key>
                            <username>{$VMWARE.USERNAME}</username>
                            <password>{$VMWARE.PASSWORD}</password>
                            <applications>
                                <application>
                                    <name>VMWare</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Discover VMware datastores</name>
                    <type>SIMPLE</type>
                    <key>vmware.datastore.discovery[{$VMWARE.URL}]</key>
                    <username>{$VMWARE.USERNAME}</username>
                    <password>{$VMWARE.PASSWORD}</password>
                    <item_prototypes>
                        <item_prototype>
                            <name>Free space on datastore {#DATASTORE} (percentage)</name>
                            <type>SIMPLE</type>
                            <key>vmware.datastore.size[{$VMWARE.URL},{#DATASTORE},pfree]</key>
                            <history>7d</history>
                            <trends>14d</trends>
                            <value_type>FLOAT</value_type>
                            <units>%</units>
                            <username>{$VMWARE.USERNAME}</username>
                            <password>{$VMWARE.PASSWORD}</password>
                            <applications>
                                <application>
                                    <name>VMWare</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Total size of datastore {#DATASTORE}</name>
                            <type>SIMPLE</type>
                            <key>vmware.datastore.size[{$VMWARE.URL},{#DATASTORE}]</key>
                            <history>7d</history>
                            <trends>14d</trends>
                            <units>B</units>
                            <username>{$VMWARE.USERNAME}</username>
                            <password>{$VMWARE.PASSWORD}</password>
                            <applications>
                                <application>
                                    <name>VMWare</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Discover VMware hypervisors</name>
                    <type>SIMPLE</type>
                    <key>vmware.hv.discovery[{$VMWARE.URL}]</key>
                    <username>{$VMWARE.USERNAME}</username>
                    <password>{$VMWARE.PASSWORD}</password>
                    <host_prototypes>
                        <host_prototype>
                            <host>{#HV.UUID}</host>
                            <name>{#HV.NAME}</name>
                            <group_links>
                                <group_link>
                                    <group>
                                        <name>Templates</name>
                                    </group>
                                </group_link>
                            </group_links>
                            <group_prototypes>
                                <group_prototype>
                                    <name>{#CLUSTER.NAME}</name>
                                </group_prototype>
                                <group_prototype>
                                    <name>{#DATACENTER.NAME}</name>
                                </group_prototype>
                            </group_prototypes>
                            <templates>
                                <template>
                                    <name>Zoes Collective Template</name>
                                </template>
                            </templates>
                            <macros>
                                <macro>
                                    <macro>{$VMWARE.HV.UUID}</macro>
                                    <value>{#HV.UUID}</value>
                                    <description>UUID of hypervisor.</description>
                                </macro>
                            </macros>
                            <custom_interfaces>YES</custom_interfaces>
                            <interfaces>
                                <interface>
                                    <ip>{#HV.IP}</ip>
                                </interface>
                            </interfaces>
                        </host_prototype>
                    </host_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Discover VMware VMs</name>
                    <type>SIMPLE</type>
                    <key>vmware.vm.discovery[{$VMWARE.URL}]</key>
                    <username>{$VMWARE.USERNAME}</username>
                    <password>{$VMWARE.PASSWORD}</password>
                    <description>Discovery of guest virtual machines.</description>
                    <host_prototypes>
                        <host_prototype>
                            <host>{#VM.UUID}</host>
                            <name>{#VM.NAME}</name>
                            <group_links>
                                <group_link>
                                    <group>
                                        <name>Templates</name>
                                    </group>
                                </group_link>
                            </group_links>
                            <templates>
                                <template>
                                    <name>Zoes Collective Template</name>
                                </template>
                            </templates>
                            <macros>
                                <macro>
                                    <macro>{$VMWARE.VM.UUID}</macro>
                                    <value>{#VM.UUID}</value>
                                    <description>UUID of guest virtual machine.</description>
                                </macro>
                            </macros>
                            <custom_interfaces>YES</custom_interfaces>
                            <interfaces>
                                <interface>
                                    <ip>{#VM.IP}</ip>
                                </interface>
                            </interfaces>
                        </host_prototype>
                    </host_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Mounted filesystem discovery</name>
                    <type>SIMPLE</type>
                    <key>vmware.vm.vfs.fs.discovery[{$VMWARE.URL},{$VMWARE.VM.UUID}]</key>
                    <delay>1h</delay>
                    <username>{$VMWARE.USERNAME}</username>
                    <password>{$VMWARE.PASSWORD}</password>
                    <filter>
                        <evaltype>AND</evaltype>
                        <conditions>
                            <condition>
                                <macro>{#FSDRIVETYPE}</macro>
                                <value>{$VFS.FS.FSDRIVETYPE.MATCHES}</value>
                                <formulaid>A</formulaid>
                            </condition>
                            <condition>
                                <macro>{#FSDRIVETYPE}</macro>
                                <value>{$VFS.FS.FSDRIVETYPE.NOT_MATCHES}</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>B</formulaid>
                            </condition>
                            <condition>
                                <macro>{#FSNAME}</macro>
                                <value>{$VFS.FS.FSNAME.MATCHES}</value>
                                <formulaid>C</formulaid>
                            </condition>
                            <condition>
                                <macro>{#FSNAME}</macro>
                                <value>{$VFS.FS.FSNAME.NOT_MATCHES}</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>D</formulaid>
                            </condition>
                            <condition>
                                <macro>{#FSTYPE}</macro>
                                <value>{$VFS.FS.FSTYPE.MATCHES}</value>
                                <formulaid>E</formulaid>
                            </condition>
                            <condition>
                                <macro>{#FSTYPE}</macro>
                                <value>{$VFS.FS.FSTYPE.NOT_MATCHES}</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>F</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <description>Discovery of all guest file systems.</description>
                    <item_prototypes>
                        <item_prototype>
                            <name>VMware: Free disk space on {#FSNAME}</name>
                            <type>SIMPLE</type>
                            <key>vmware.vm.vfs.fs.size[{$VMWARE.URL},{$VMWARE.VM.UUID},{#FSNAME},free]</key>
                            <delay>1h</delay>
                            <history>7d</history>
                            <trends>30d</trends>
                            <units>B</units>
                            <username>{$VMWARE.USERNAME}</username>
                            <password>{$VMWARE.PASSWORD}</password>
                        </item_prototype>
                        <item_prototype>
                            <name>VMware: Free disk space on {#FSNAME} (percentage)</name>
                            <type>SIMPLE</type>
                            <key>vmware.vm.vfs.fs.size[{$VMWARE.URL},{$VMWARE.VM.UUID},{#FSNAME},pfree]</key>
                            <delay>1h</delay>
                            <history>7d</history>
                            <trends>30d</trends>
                            <value_type>FLOAT</value_type>
                            <units>%</units>
                            <username>{$VMWARE.USERNAME}</username>
                            <password>{$VMWARE.PASSWORD}</password>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{min(#5)}&lt;=2</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{min(5)}&gt;2</recovery_expression>
                                    <name>Free disk space is less than 2% on {#FSNAME}</name>
                                    <priority>DISASTER</priority>
                                    <description>Available drive storage has fallen below 2%. You do not need to manually close this item. Get on the device and return available storage to greater than 2% and the alert will auto resolve.</description>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{min(#5)}&lt;=10</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{min(5)}&gt;10</recovery_expression>
                                    <name>Free disk space is less than 10% on {#FSNAME}</name>
                                    <priority>HIGH</priority>
                                    <description>Available drive storage has fallen below 10%. You do not need to manually close this item. Get on the device and return available storage to greater than 2% and the alert will auto resolve.</description>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{min(#5)}&lt;=15</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{min(5)}&gt;15</recovery_expression>
                                    <name>Free disk space is less than 15% on {#FSNAME}</name>
                                    <priority>AVERAGE</priority>
                                    <description>Available drive storage has fallen below 15%. You do not need to manually close this item. Get on the device and return available storage to greater than 2% and the alert will auto resolve.</description>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{min(#5)}&lt;=20</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{min(5)}&gt;20</recovery_expression>
                                    <name>Free disk space is less than 20% on {#FSNAME}</name>
                                    <priority>WARNING</priority>
                                    <description>Available drive storage has fallen below 20%. You do not need to manually close this item. Get on the device and return available storage to greater than 2% and the alert will auto resolve.</description>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>VMware: Total disk space on {#FSNAME}</name>
                            <type>SIMPLE</type>
                            <key>vmware.vm.vfs.fs.size[{$VMWARE.URL},{$VMWARE.VM.UUID},{#FSNAME},total]</key>
                            <delay>1h</delay>
                            <history>7d</history>
                            <trends>30d</trends>
                            <units>B</units>
                            <username>{$VMWARE.USERNAME}</username>
                            <password>{$VMWARE.PASSWORD}</password>
                        </item_prototype>
                        <item_prototype>
                            <name>VMware: Used disk space on {#FSNAME}</name>
                            <type>SIMPLE</type>
                            <key>vmware.vm.vfs.fs.size[{$VMWARE.URL},{$VMWARE.VM.UUID},{#FSNAME},used]</key>
                            <history>7d</history>
                            <trends>30d</trends>
                            <units>B</units>
                            <username>{$VMWARE.USERNAME}</username>
                            <password>{$VMWARE.PASSWORD}</password>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$URLQBITTORRENT}</macro>
                    <value></value>
                </macro>
                <macro>
                    <macro>{$VMWARE.PASSWORD}</macro>
                    <description>VMware service {$USERNAME} user password</description>
                </macro>
                <macro>
                    <macro>{$VMWARE.URL}</macro>
                    <description>VMware service (vCenter or ESX hypervisor) SDK URL (https://servername/sdk)</description>
                </macro>
                <macro>
                    <macro>{$VMWARE.USERNAME}</macro>
                    <description>VMware service user name</description>
                </macro>
                <macro>
                    <macro>{$VMWARE.VM.UUID}</macro>
                    <value></value>
                    <description>VM UUID</description>
                </macro>
            </macros>
            <valuemaps>
                <valuemap>
                    <name>VirtualMachinePowerState</name>
                    <mappings>
                        <mapping>
                            <value>0</value>
                            <newvalue>poweredOff</newvalue>
                        </mapping>
                        <mapping>
                            <value>1</value>
                            <newvalue>poweredOn</newvalue>
                        </mapping>
                        <mapping>
                            <value>2</value>
                            <newvalue>suspended</newvalue>
                        </mapping>
                    </mappings>
                </valuemap>
            </valuemaps>
        </template>
    </templates>
</zabbix_export>
